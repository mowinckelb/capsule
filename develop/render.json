{
  "version": 2,
  "builds": [
    {"src": "app.py", "use": "@render/python"}
  ],
  "env": {
    "PYTHONPATH": "./"
  },
  "services": [
    {
      "type": "web",
      "env": "python",
      "runtimeVersion": "python3.12",
      "buildCommand": "pip install -r requirements.txt",
      "startCommand": "python app.py"
    }
  ]
}
